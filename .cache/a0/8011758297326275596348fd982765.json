onst playStop = document.getElementById('playStatus');\nplayButton.onclick = () => {\n  ready = !ready;\n  ready ? osc.start() : osc.stop();\n  playStop.textContent = ready ? 'STOP' : 'PLAY';\n};\n"},"lineCount":null}},"error":null,"hash":"1c4720199f68b75b28f393d98326fe9e","cacheData":{"env":{}}}